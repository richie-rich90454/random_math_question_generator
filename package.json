  "dependencies": {
    "@fastify/static": "^8.2.0",
    "cssnano": "^7.1.2",
    "fastify": "^5.3.2",
    "mathjax": "^3.2.2",
    "mathjs": "^15.1.0",
    "terser": "^5.39.0"
  },
  "description": "Random math question generator made with HTML, CSS, and JS.",
  "devDependencies": {
    "electron": "^36.4.0",
    "electron-builder": "^26.0.12",
    "vite": "^7.3.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-html": "^3.2.2",
